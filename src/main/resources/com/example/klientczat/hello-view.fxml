<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.klientczat.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TextField fx:id="loginText" maxWidth="-Infinity" prefHeight="25.0" prefWidth="155.0" />
      <PasswordField fx:id="passwordText" maxWidth="-Infinity" />
      <Button mnemonicParsing="false" onAction="#login" text="Button" />
   </children>
</VBox>
